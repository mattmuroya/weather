{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        // "no-console": "off"
        "eol-last": ["error", "never"],
        "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
        "no-use-before-define": ["error", {"functions": false, "classes": false}]
    }
}
